{
  "name": "dashboard",
  "version": "0.1.0",
  "description": "Rubyx dashboard",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build -i src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@uiw/react-md-editor": "^3.20.2",
    "@windmill/react-ui": "0.3.1",
    "@heroicons/react": "2.0.17",
    "react-icons": "4.8.0",
    "axios": "^1.1.3",
    "lodash": "^4.17.21",
    "chart.js": "^2.9.4",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "postcss": "^8.4.21",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.1",
    "react-transition-group": "4.4.1",
    "redux": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "redux-thunk": "^2.4.2",
    "rehype-sanitize": "^5.0.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^9",
    "commitizen": "^4.3.0",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "10.1.0",
    "react-axe": "3.5.2",
    "release-it": "^15.6.0",
    "tailwindcss": "3.2.7"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
